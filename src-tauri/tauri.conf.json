{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Q-Zip",
  "version": "0.1.2",
  "identifier": "com.creepio.q-zip",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm generate"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Q-Zip v0.1.2",
        "width": 1920,
        "height": 1080,
        "resizable": true,
        "fullscreen": false,
        "dragDropEnabled": true,
        "decorations": false
      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}